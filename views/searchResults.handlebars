{{!-- from public js: rendered after search button is clicked, populated by github search results --}}
{{!-- results are clickable and take the user to 'newNote' page to be viewed/saved --}}
<div>
    {{#each resultObject}}
    <div class="card text-center p-4 mb-4 bg-transparent border-opacity-10">
    <div class="card-body">
    <a href="/results/1"><h2 class="card-title">{{name}}</h2></a>
    <p class="card-text">{{description}}</p>
    <a href="{{link}}" class="btn btn-primary">Go to Repo</a>
  </div>
</div>

    {{/each}}
</div>

