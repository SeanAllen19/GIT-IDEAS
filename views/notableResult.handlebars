<div>
    {{!-- displays repo information and form for submitting note with comment and tag, once saved, take user back to searchresults --}}
    <div>
        <h1>{{objectOne.name}}</h1>
        <p></p>
    </div>
    {{!-- if logged-in --}}
    <div>
        <form>
            <div class="mb-3">
                <input type="comment" placeholder="Add a comment" class="form-control" id="comment" aria-describedby="comment">
            </div>
            <div class="mb-3">
                <label for="newTag" class="form-label">New Tag:</label>
                <input type="newTag" class="form-control" id="newTag" aria-describedby="newTag">
                <select class="form-select" aria-label="Default select example">
                    <option selected>Choose a Tag</option>
                    {{#each tags}}
                    <option value="{{id}}">{{title}}</option>
                    {{/each}}
                </select>
            </div>
            <button type="submit" class="btn btn-primary">Save</button>
        </form>
    </div>
    {{!-- /if --}}

    <a href="/results"><button class="btn btn-outline-success" id="back-to-results">Back to Results</button></a>
</div>

